# WasmWiz Environment Configuration - Development

# Database Configuration (using local PostgreSQL)
DATABASE_URL=postgresql://wasmwiz:wasmwiz_dev@localhost:5432/wasmwiz

# Server Configuration
SERVER_HOST=127.0.0.1
SERVER_PORT=8080

# Logging Configuration
RUST_LOG=info,wasmwiz=debug

# Security Configuration
API_SALT=development-salt-not-for-production

# File Storage Configuration
WASM_TEMP_DIR=/tmp/wasm_modules

# Resource Limits
MAX_WASM_SIZE=10485760      # 10MB in bytes
MAX_INPUT_SIZE=1048576      # 1MB in bytes

# Execution Limits
MAX_MEMORY_MB=128
MAX_EXECUTION_TIME_SECONDS=5
